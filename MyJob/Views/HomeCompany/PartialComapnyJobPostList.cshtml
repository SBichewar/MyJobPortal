@model IEnumerable< DAL.PostJobMst>

@foreach (var item in Model)
{
    <div class="row" style="background-color: #ffeb3b0a;box-shadow: 0 1px 1px 0 rgb(44, 58, 1), 0 1px 2px 0 rgb(25, 66, 2), 0 2px 1px -2px rgba(0,0,0,.2), 0 1px 1px 0 rgba(0,0,0,.05);
    border-radius: 23px;
    padding: 22px;">
        <div class="col-md-12 pull-right">
            <a href="@Url.Action("DeleteJobPost", "HomeCompany",@item)" class="pull-right" > <span><i class="fa fa-trash-o" aria-hidden="true"></i></span> Delete</a>
            <a href="#" class="pull-right" data-toggle="modal" data-target="#ModalUpdatePost"> <span><i class="fa fa-pencil" aria-hidden="true"></i></span> Update</a>
        </div>
        <div class="col-md-12 ">
            <div><h3 class="bg-success">@item.JobCategory</h3></div>
            <div><h6>@item.ExprienceRequire</h6></div>
            <div>@item.RequireSkill</div>
            <div>@item.PostDiscription</div>
        </div>
    </div>
}

