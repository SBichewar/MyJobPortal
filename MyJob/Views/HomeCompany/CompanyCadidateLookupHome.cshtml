
@{
    ViewBag.Title = "CompanyCadidateLookupHome";
    Layout = "~/Views/HomeCompany/_CompayCandidateLookupLayour.cshtml";
}
<link href="~/css/jquery.dataTables.min.css" rel="stylesheet" />
<script src="~/js/jquery.min.js"></script>
<script src="~/js/jquery.dataTables.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $.ajax({
            type: "GET",
            url: "http://localhost:53763/HomeCompany/GetJobSeackerList",
            datatype: "json",
            traditional: true,
            success: function (data) {
                if (data != null) {
                    var p_Upcomingobj = JSON.parse(data);
                    $('#JobSeakerGrid').DataTable({
                        data: p_Upcomingobj,
                        columns: [
                            { data: 'Fname' },
                            { data: 'Lname' },
                            { data: 'Address' },
                            { data: 'City' },
                             { data: 'State' },
                             { data: 'Gender' },
                             { data: 'Mobile' },
                             { data: 'EmailId' },
                             { data: 'EntryDate' },
                             { data: 'Resume' }]
                    });
                }
            }
        });
    });

</script>

<div class="row " style="background-color: #ffeb3b0a;box-shadow: 0 1px 1px 0 rgb(44, 58, 1), 0 1px 2px 0 rgb(25, 66, 2), 0 2px 1px -2px rgba(0,0,0,.2), 0 1px 1px 0 rgba(0,0,0,.05);
    border-radius: 23px;
    padding: 22px;">
    <div class="col-md-12 text-primary">
        @{Html.RenderAction("PartialCompanyHeadInfor", "HomeCompany");}
    </div>
</div>
<div class="row " style="background-color: #ffeb3b0a;box-shadow: 0 1px 1px 0 rgb(44, 58, 1), 0 1px 2px 0 rgb(25, 66, 2), 0 2px 1px -2px rgba(0,0,0,.2), 0 1px 1px 0 rgba(0,0,0,.05);
    border-radius: 23px;
    padding: 22px;">
    <div class="col-md-12 text-primary">
        <table id="JobSeakerGrid" class="display" style="width:100%">
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Address</th>
                    <th>City</th>
                    <th>State</th>
                    <th>Gender</th>
                    <th>Mobile</th>
                    <th>EmailId</th>
                    <th>EntryDate</th>
                    <th>Resume</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
</div>
